x=int(input())
for i in range(0,x):
    b=1
    z=0
    n=int(input())
    for b in range(1,n):
        a=(n*(n-2*b))//(2*(n-b))
        c=n-(a+b)
        if(a**2+b**2==c**2 and a>0 and b>0 and c>0 and (a<b and b<c)):
            print(a*b*c)
            z=1
            break;
    if(z==0):
        print(-1)
